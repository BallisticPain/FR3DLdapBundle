{
    "name": "fr3d/ldap-symfony-bundle",
    "type": "symfony-bundle",
    "description": "This package provide users and authentication services based on LDAP directories for Symfony2 framework",
    "keywords": ["ldap", "authentication"],
    "version": "2.0.0-ALPHA3",
    "license": "MIT",
    "authors": [
        {
            "name": "Maks3w"
        }
    ],
    "repositories": {
        "zend-framework-2": {
            "package": {
                "name": "zend/zf2",
                "version": "master-dev",
                "source": {
                    "type": "git",
                    "url": "http://github.com/zendframework/zf2.git",
                    "reference": "master-dev"
                },
                "autoload": {
                    "psr-0": {
                        "Zend\\": "library/"
                    }
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.2",
        "ext-ldap": "*",
        "symfony/config": "2.0.*",
        "symfony/dependency-injection": "2.0.*",
        "symfony/security": "2.0.*",
        "symfony/security-bundle": "2.0.*"
    },
    "recommend": {
        "symfony/class-loader": "2.0.*",
        "symfony/validator": "2.0.*"
    },
    "suggest": {
        "zend/zf2": "master-dev",
        "symfony/framework-bundle": "2.0.*",
        "friendsofsymfony/user-bundle": "*"
    },
    "autoload": {
        "psr-0": { "FR3D\\LdapBundle": "" }
    },
    "target-dir": "FR3D/LdapBundle"
}
